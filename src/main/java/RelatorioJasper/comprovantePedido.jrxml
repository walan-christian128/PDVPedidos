	<parameter name="id_pedido" class="java.lang.Integer"/>
	<parameter name="id_empresa" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
			pedidos.id_pedido AS codigo,
			tb_cliente_pedido.nome AS Nome,
			tb_cliente_pedido.telefone AS Celular,
			DATE_FORMAT(NOW(),'%d/%m/%Y') AS data,
			tb_cliente_pedido.endereco AS Endereco,
			tb_cliente_pedido.numero AS Numero,
			tb_cliente_pedido.bairro AS Bairro,
			tb_cliente_pedido.cidade AS Cidade,
			tb_produtos.descricao AS Descricao,
			tb_produtos.preco_de_venda AS preco,
			pedidos.total_pedido AS total_pedido,
			itens_pedido.quantidade AS quantidade,
			tb_empresa.endereco AS empresa,
			tb_empresa.nome AS NomeEmpresa,
			tb_empresa.logo AS logo
		FROM pedidos
		INNER JOIN itens_pedido ON pedidos.id_pedido = itens_pedido.pedido_id
		LEFT JOIN tb_cliente_pedido ON pedidos.clientepedido_id = tb_cliente_pedido.id
		INNER JOIN tb_empresa ON tb_empresa.id = pedidos.empresa_id
		INNER JOIN tb_produtos ON tb_produtos.id = itens_pedido.produto_id
		WHERE
			pedidos.id_pedido = $P{id_pedido}
			AND tb_empresa.id = $P{id_empresa}
		ORDER BY tb_produtos.descricao ASC]]>
	</queryString>
